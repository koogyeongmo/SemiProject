<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper  namespace="imageMapper">
	
    <insert id="uploadImage" parameterType="ImageDto">
        INSERT INTO image_table (image_id, image_blob) VALUES (#{imageId}, #{imageBlob})
    </insert>
    
	<select id="retrieveImage" parameterType="string" resultType="ImageDto">
		SELECT image_id, image_blob FROM image_table WHERE image_id=#{imageId}
    </select>
</mapper>